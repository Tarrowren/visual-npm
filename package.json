{
	"name": "visual-npm",
	"displayName": "visual npm",
	"description": "npm 可视化界面",
	"version": "0.0.1",
	"publisher": "Tarrow",
	"license": "MIT",
	"keywords": [
		"npm",
		"visualization"
	],
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld"
	],
	"main": "./out/extension.js",
